

docker run -it \
  --name pg-database \
  -e POSTGRES_USER=root\
  -e POSTGRES_PASSWORD=root\
  -e POSTGRES_DB=ny_taxi \
  -v D:/Development/Data-Engineering/Docker/ny_taxi_postgres_data:/var/lib/postgresql/data\
  -p 5432:5432 \
  postgres:13


docker run -it --name pg-db -e POSTGRES_USER=test -e POSTGRES_PASSWORD=test -e POSTGRES_DB=ny_taxi -v D:/Development/Data-Engineering/Docker/ny_taxi_postgres_data:/var/lib/postgresql/data -p 6543:5432 postgres:13